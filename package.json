{
  "name": "@maskedeng-tom/ssrsx",
  "version": "0.1.42",
  "description": "server side renderer with tsx(jsx)",
  "keywords": [
    "html",
    "ssr",
    "server",
    "side",
    "rendering",
    "jsx",
    "tsx",
    "koa"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Maskedeng Tom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maskedeng-tom/ssrsx.git"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc --project tsconfig.build.json && npm run modDist && cp -r client dist",
    "modDist": "echo '/// <reference path=\"../ssrsx.d.ts\" />' > data.tmp; cat dist/index.d.ts >> data.tmp; cat data.tmp > dist/index.d.ts && rm data.tmp",
    "prepublishOnly": "npm version patch && npm run build",
    "start": "nodemon",
    "test": "jest"
  },
  "dependencies": {
    "@types/mime-types": "^2.1.4",
    "debug": "^4.3.4",
    "mime-types": "^2.1.35",
    "path-to-regexp": "^6.2.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jquery": "^3.5.29",
    "@types/koa": "^2.14.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-helmet": "^6.0.8",
    "@types/koa-session": "^6.4.5",
    "@types/koa-static": "^4.0.4",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "express": "^4.18.2",
    "jest": "^29.7.0",
    "jquery": "^3.7.1",
    "jsx": "file:jsx",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-helmet": "^7.0.2",
    "koa-session": "^6.4.0",
    "koa-static": "^5.0.0",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1"
  }
}
